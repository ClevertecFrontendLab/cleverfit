{
  "name": "cleverfit",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": "20.10.0",
    "npm": "please-use-yarn",
    "yarn": "1.22.21"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:e2e": "start-server-and-test start http-get://localhost:3000 cy:open-e2e",
    "test": "vitest"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "classnames": "^2.3.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.2",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/css-modules": "^1.0.5",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.6.1",
    "eslint": "^8.55.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "prettier": "^3.1.0",
    "start-server-and-test": "^2.0.3",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^5.0.2",
    "vite": "^5.0.6",
    "vitest": "^1.0.2"
  },
  "packageManager": "yarn@1.22.21"
}
